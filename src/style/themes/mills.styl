millsTheme = {
  tileFill: #FBC02D,
  possible: #FFE57F,
  tileStroke: #212121,
  lineStroke: #212121,
  waiting: #E0E0E0,
  waitingDark: #616161,
  tileWaiting: #A1887F,
  t1: #000,
  t2: #fff,
  s1: #000,
  s2: #fff,
  l1: #3E2723,
  l2: #3E2723,
}

@require "./colors"

colors(millsTheme, '.mills-theme')

.mills-theme
  polygon:not(.possible)
    fill: #FBC02D !important
  line
    shape-rendering: crispEdges
