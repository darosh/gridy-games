  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "private": true,
  "scripts": {
    "clean": "del-cli dist/*.js dist/*.html dist/static",
    "build": "webpack --mode production -p",
    "deploy": "npm run clean && npm --no-git-tag-version version patch && npm run build && deploy-to-gh-pages --local ./dist",
    "dev": "webpack --mode development",
    "eslint": "eslint --fix --ext .js,.vue src",
    "install": "rollup -c rollup.config.js -f es -o plugins/icons.js plugins/icons.bundle.js && cross-env NODE_ENV=es5 babel build/vuetify/src --out-dir build/vuetify/es5 && node ./build/patch.js vuetify",
    "report": "npm run build --report",
    "start": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --open",
    "stats": "webpack --profile --json > stats.json",
    "tslint": "tslint --fix -p tsconfig.json"
  }
}
