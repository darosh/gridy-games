{
  "name": "gridy-games",
  "description": "Board games",
  "version": "0.1.152",
  "author": "Jan Forst <ensonador@gmail.com>",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11",
    "not op_mini all",
    "not ie_mob <= 100",
    "not op_mob <= 30",
    "not android < 60",
    "not samsung < 6"
  ],
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "d3-scale": "^2.0.0",
    "fscreen": "^1.0.2",
    "gridy": "github:darosh/gridy",
    "intersection-observer": "^0.5.0",
    "lunr": "^2.1.6",
    "material-icons-bundle": "github:darosh/material-icons-bundle",
    "tone": "^0.12.72",
    "vue": "^2.5.16",
    "vue-observe-visibility": "^0.3.1",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-detective": "^2.0.0",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cli-real-favicon": "^0.0.6",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "del-cli": "^1.1.0",
    "deploy-to-gh-pages": "^1.3.6",
    "eslint": "^4.19.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.5",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "github:webpack-contrib/html-webpack-plugin",
    "inject-loader": "^3.0.1",
    "postcss-loader": "^2.1.2",
    "recursive-readdir": "^2.2.2",
    "rollup": "^0.57.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "style-loader": "^0.20.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-loader": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "uglify-es": "^3.3.10",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.1.1",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.0.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "worker-loader": "^1.1.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "iconVersion": "0.6.0",
  "storageVersion": "0.1.144",
  "private": true,
  "scripts": {
    "build": "webpack --mode production -p",
    "build:dev": "webpack --mode development",
    "build:report": "npm run build --report",
    "build:stats": "webpack --profile --json > stats.json",
    "clean": "del-cli dist/*.js dist/*.html dist/static",
    "deploy": "npm run clean && npm --no-git-tag-version version patch && npm run build && deploy-to-gh-pages --local ./dist",
    "install": "rollup -c rollup.config.js -f es -o plugins/icons.js plugins/icons.bundle.js && cross-env NODE_ENV=es5 babel build/vuetify/src --out-dir build/vuetify/es5 && node ./build/patch.js vuetify",
    "icons": "real-favicon generate icon.config.json .icons/data.json .icons",
    "lint:es": "eslint -f unix --fix --ext .js,.vue src",
    "lint:ts": "tslint --fix -p tsconfig.json",
    "start": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --open --host 0.0.0.0 --useLocalIp"
  }
}
